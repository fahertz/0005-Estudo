   "cell_type": "code",
   "execution_count": 45,
   "metadata": {},
   "outputs": [],
   "source": [
    "def encontreCachorro(str):\n",
    "    if 'cachorro' in str.lower():\n",
    "        return True\n",
    "    else:\n",
    "        return False"
